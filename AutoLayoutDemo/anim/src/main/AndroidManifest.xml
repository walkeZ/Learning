<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.walke.anim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".coin_wallet.CoinWalletActivity"/>
        <activity android:name=".point.PointFallActivity"/>
        <activity android:name=".springback.SpringbackActivity"/>
        <activity android:name=".explosions.ParticleExplosionsActivity"/>
        <activity android:name=".fireworks.FireworksEditTextActivity"/>
    </application>

</manifest>